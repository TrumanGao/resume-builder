{
  "name": "resume-builder",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite/vite.development.ts",
    "build": "rimraf dist && vite build --config vite/vite.production.ts",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "element-plus": "^2.6.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "trumangao-utils": "^2.2.11",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "fs-extra": "^11.2.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vue-tsc": "^2.0.6"
  },
  "author": "TrumanGao",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrumanGao/resume-builder.git"
  },
  "homepage": "https://trumangao.github.io/resume-builder/dist/#/resume/aamirkhan",
  "license": "MIT",
  "keywords": [
    "resume"
  ]
